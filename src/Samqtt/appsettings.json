{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "samqtt": {
    "Broker": {
      "Server": "192.168.10.4",
      "Port": 1883,
      "Username": "",
      "Password": ""
    },
    "DeviceIdentifier": "testapp",
    "TimerInterval": 5,
    "Sensors": {
      "Timestamp": {
        "enabled": true
      },
      "CpuProcessorTime": {
        "enabled": true
      },
      "FreeMemory": {
        "enabled": true
      },
      "NetworkAvailability": {
        "enabled": true
      },
      "ComputerInUse": {
        "enabled": false
      }
    },
    "MultiSensors": {
      "Drive": {
        "enabled": true,
        "Sensors": {
          "DriveFreeSize": {
            "enabled": true
          },
          "DrivePercentFreeSize": {
            "enabled": true
          },
          "DriveTotalSize": {
            "enabled": true
          }
        }
      }
    },
    "Actions": {
      "Hibernate": {
        "Enabled": true
      },
      "Reboot": {
        "Enabled": true
      },
      "Shutdown": {
        "Enabled": true
      },
      "Suspend": {
        "Enabled": true
      },
      "SendNotification": {
        "Enabled": true
      },
      "StartProcess": {
        "Enabled": true
      },
      "KillProcess": {
        "Enabled": true
      },
      "GetProcesses": {
        "Enabled": true
      },
      "GetProcess": {
        "Enabled": true
      }
    }
  }
}