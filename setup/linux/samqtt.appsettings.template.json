{
    "samqtt": {
        "Broker": {
            "Server": "{{MQTT_SERVER}}",
            "Port": {{MQTT_PORT}},
            "Username": "{{MQTT_USERNAME}}",
            "Password": "{{MQTT_PASSWORD}}"
        },
        "DeviceIdentifier": "{{DEVICE_IDENTIFIER}}",
        "TimerInterval": 5,
        "Sensors": {
            "Timestamp": {
                "enabled": true
            },
            "CpuProcessorTime": {
                "enabled": true
            },
            "FreeMemory": {
                "enabled": true
            },
            "NetworkAvailability": {
                "enabled": true
            },
            "ComputerInUse": {
                "enabled": false
            }
        },
        "MultiSensors": {
            "Drive": {
                "enabled": false,
                "Sensors": {
                    "DriveFreeSize": {
                        "enabled": false
                    },
                    "DrivePercentFreeSize": {
                        "enabled": false
                    },
                    "DriveTotalSize": {
                        "enabled": false
                    }
                }
            }
        },
        "Actions": {
            "Hibernate": {
                "Enabled": false
            },
            "Reboot": {
                "Enabled": false
            },
            "Shutdown": {
                "Enabled": false
            },
            "Suspend": {
                "Enabled": false
            },
            "SendNotification": {
                "Enabled": false
            },
            "StartProcess": {
                "Enabled": true
            },
            "KillProcess": {
                "Enabled": false
            },
            "GetProcesses": {
                "Enabled": true
            },
            "GetProcess": {
                "Enabled": true
            }
        }
    }
}
